{
  "name": "littlellm",
  "version": "3.8.0",
  "description": "A powerful desktop AI chat application with enterprise-grade security, encrypted API key storage, and advanced features",
  "author": "LittleLLM Team",
  "private": true,
  "type": "module",
  "main": "dist/electron/electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/fix-paths.js && node scripts/copy-assets.cjs",
    "build-no-lint": "next build --no-lint && node scripts/fix-paths.js",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/agentic-workflows.test.ts",
    "test:integration": "vitest run tests/integration/",
    "test:performance": "vitest run tests/performance/",
    "test:scenarios": "vitest run tests/scenarios/",
    "test:stress": "vitest run tests/stress/",
    "test:smoke": "vitest run tests/agentic-workflows.test.ts --reporter=basic",
    "test:standard": "vitest run tests/agentic-workflows.test.ts tests/integration/ tests/scenarios/",
    "test:full": "vitest run --reporter=verbose",
    "test:ci": "vitest run tests/agentic-workflows.test.ts tests/integration/ --reporter=json --outputFile=test-results.json",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "benchmark": "node scripts/performance-benchmark.mjs",
    "benchmark:verbose": "cross-env DEBUG_TOOLS=true node scripts/performance-benchmark.mjs",
    "electron": "npm run build-electron && npx electron .",
    "electron-dev": "concurrently \"npm run dev\" \"npm run build-electron && node scripts/wait-for-nextjs.js\"",
    "electron-pack": "npm run build && npm run build-electron && electron-builder",
    "build-electron": "tsc --project electron/tsconfig.json && tsc --project electron/tsconfig.preload.json",
    "dist": "npm run build && npm run build-electron && electron-builder --publish=never",
    "dist:win": "npm run build && npm run build-electron && electron-builder --win --publish=never",
    "dist:win-installer": "npm run build && npm run build-electron && electron-builder --win --config.win.target=nsis --publish=never",
    "dist:win-portable": "npm run build && npm run build-electron && electron-builder --win --config.win.target=portable --publish=never",
    "build:windows": "node scripts/build-windows.js",
    "build:mac": "node scripts/build-mac.js",
    "create-mac-icon": "node scripts/create-mac-icon.js",
    "dist:mac": "npm run build && npm run build-electron && electron-builder --mac --publish=never",
    "dist:mac-dmg": "npm run build && npm run build-electron && electron-builder --mac --config.mac.target=dmg --publish=never",
    "dist:mac-zip": "npm run build && npm run build-electron && electron-builder --mac --config.mac.target=zip --publish=never",
    "dist:mac-universal": "npm run build && npm run build-electron && electron-builder --mac --universal --publish=never",
    "dist:linux": "npm run build && npm run build-electron && electron-builder --linux --publish=never"
  },
  "dependencies": {
    "@apache-arrow/ts": "^14.0.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/mime-types": "^3.0.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xenova/transformers": "^2.17.2",
    "apache-arrow": "^14.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "critters": "^0.0.23",
    "csv-parser": "^3.2.0",
    "electron-is-dev": "^3.0.1",
    "flatbuffers": "^23.5.26",
    "framer-motion": "^12.23.9",
    "ical.js": "^2.2.0",
    "lucide-react": "^0.352.0",
    "mammoth": "^1.9.1",
    "mime-types": "^3.0.1",
    "next": "14.1.0",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^7.0.1",
    "node-loader": "^2.1.0",
    "node-pptx-parser": "^1.0.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.54",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "rtf-parser": "^1.3.3",
    "sharp": "^0.34.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vectordb": "^0.21.1",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.1.0",
    "electron-builder": "^25.1.8",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "svg2img": "^1.0.0-beta.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  },
  "optionalDependencies": {
    "@lancedb/vectordb-darwin-arm64": "^0.21.1",
    "@lancedb/vectordb-darwin-x64": "^0.21.1",
    "@lancedb/vectordb-linux-x64-gnu": "^0.21.1",
    "@lancedb/vectordb-win32-x64-msvc": "^0.21.1"
  }
}
